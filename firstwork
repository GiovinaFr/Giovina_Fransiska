module One where

import Data.List

-- 1.a

null' x
  | x == [] = True
  | otherwise = False

-- pembatas

fst' (a,b) = a

-- pembatas

snd' (a,b) = b

-- pembatas

and' x
  | x == [True,True] = True
  | otherwise = False

-- pembatas

or' x
  | x == [True,True] = True
  | x == [True,False] = True
  | x == [False, True] = True
  | otherwise = False

